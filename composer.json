{
    "name": "mosamirzz/swoole-grpc",
    "description": "Swoole GRPC framework for building high performance services",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "OpenSwoole Group",
            "email": "hello@openswoole.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-swoole": ">=5.0"
    },
    "require-dev": {
        "swoole/ide-helper": "^5.1",
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.6",
        "friendsofphp/php-cs-fixer": "^3.6"
    },
    "suggest": {
        "ext-swoole": "*",
        "google/protobuf": "^3.21"
    },
    "autoload": {
        "psr-4": {
            "OpenSwoole\\GRPC\\": "src/"
        }
    },
    "scripts": {
        "test": [
            "psalm --no-cache"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
